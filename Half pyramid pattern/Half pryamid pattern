#include <iostream>
using namespace std;
int main() {
    int n; // The number of rows in the pattern
    cin>>n;
    // Outer loop for each row
    for (int i = 1; i <= n; ++i) {
        // Inner loop for printing numbers in each row
        for (int j = 1; j <= i; ++j) {
            cout << j << " ";
        }
        cout << std::endl; // Move to the next line after each row
    }

    return 0;
}
