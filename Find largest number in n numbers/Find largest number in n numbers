#include<iostream>
#include<climits>
using namespace std;
int main()
{
    int n,largest,i,no;
    largest=INT_MIN;
    cout<<"Enter value of n:";
    cin>>n;
    
  i=1;
  while(i<=n)
  {
      cin>>no;
      if(no>largest)
      {
          largest=no;
      }
    
    i=i+1;
    
  }
  cout<<"largest number:"<<largest<<'\n';
  return 0;
}
