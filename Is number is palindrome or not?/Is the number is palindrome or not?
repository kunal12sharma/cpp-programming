#include <iostream>
using namespace std;

int main(){

int n,b;
cout<<"Enter value of n:";
cin>>n;
int rev=0;
n=b;
    while(b!=0)
    {
        int rem= b%10;
        rev=rev*10+rem;
        b=b/10;
    }
    if(b==rev)
    {
        cout<<"The number is:palindrone";
    }
    else
    {
        cout<<"The number isnot  palidrone";
    }
return 0;
}
