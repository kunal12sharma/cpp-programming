#include <iostream>
using namespace std;
int main() {
    int n;  // Number of rows
  cin>>n;
    for (int i = 1; i <= n; ++i) {
        // Print increasing numbers
        for (int j = 1; j <= i; ++j) {
            std::cout << j;
        }

        // Print asterisks
        for (int k = n; k > i; --k) {
            std::cout << "*";
        }

        // Move to the next line
        std::cout << std::endl;
    }

    return 0;
}
