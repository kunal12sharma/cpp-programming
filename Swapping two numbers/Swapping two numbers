#include <bits/stdc++.h>
using namespace std;


int main()
{
    int a = 2, b = 3;

    cout << "Before swapping a = " << a << " , b = " << b
         << endl;

    
    int c;

    c = a;
    a = b;
    b = c;
    cout << "After swapping a = " << a << " , b = " << b
         << endl;

    return 0;
}
