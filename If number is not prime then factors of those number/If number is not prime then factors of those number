#include <iostream>
using namespace std;

int main(){

int n,key;
   std::cout<<"Enter the value of n:";
std::cin>>n;

for(int i=2;i<=n/2;i++)
{
    if(n%i==0)
    {
        key = 0;
        cout<<"The factors of number is:"<<i<<endl;
        
    }
    else
    {
        key = 1;
    }
}
if(key=0){
    cout<<"the value is prime";
}
else
{
    cout<<"the number is not prime";
}
return 0;
}
