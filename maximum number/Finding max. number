#include <iostream>
using namespace std;
int main() {
    int a,b,c;
    cout<<"enter the value of a b c:"<<endl;
    cin>>a>>b>>c;
    if(a>b){
        if(a>c){
            cout<<"The max. number is:"<<a;
        }
        else
        {
            cout<<"The max. number is:"<<c;
        }
    }
        
    else if(b>a)
    {
        if(b>c){
                cout<<"The max. number is:"<<b;
            }
            else
            {
                cout<<"The max. number is:"<<c;
        }
    }
    

    return 0;
}
